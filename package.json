{
  "name": "wincalx-web-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stomp/stompjs": "^5.4.4",
    "antd": "^3.26.3",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "firebase": "^7.14.2",
    "flow-bin": "^0.115.0",
    "formik": "^2.0.6",
    "html2canvas": "^1.0.0-rc.5",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "normalizr": "^3.6.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-cookie": "^4.0.1",
    "react-country-flag": "^2.3.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sockjs-client": "^1.4.0",
    "stylelint": "^12.0.0"
  },
  "scripts": {
    "start": "cross-env REACT_APP_CLIENT_ENV=development craco start",
    "start:staging": "cross-env REACT_APP_CLIENT_ENV=staging craco start",
    "start:production": "cross-env REACT_APP_CLIENT_ENV=production craco start",
    "build:dev": "cross-env REACT_APP_CLIENT_ENV=development craco build",
    "build:staging": "cross-env REACT_APP_CLIENT_ENV=staging craco build",
    "build:production": "cross-env REACT_APP_CLIENT_ENV=production craco build",
    "test": "cross-env REACT_APP_CLIENT_ENV=test craco test",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:style && npm run prettier",
    "lint:js": "eslint --cache --fix --ext .js,.jsx ./src",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "prettier": "prettier -c --write \"**/src/**\"",
    "ci:dev": "npm set progress=false && npm install && npm run build:dev",
    "ci:staging": "npm set progress=false && npm install && npm run build:staging",
    "ci:production": "npm set progress=false && npm install && npm run build:production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,scss,md,json}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "npm run lint:js",
      "git add"
    ],
    "**/*.scss": [
      "npm run lint:style",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "empty": "^0.10.1",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-unicorn": "^13.0.0",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.4.3",
    "moxios": "^0.4.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "redux-mock-store": "^1.5.4",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpackbar": "^4.0.0"
  }
}
